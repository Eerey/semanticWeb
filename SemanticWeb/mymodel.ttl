@prefix :      <http://www.co-ode.org/ontologies/pizza/pizza.owl#> .
@prefix pizza: <http://www.co-ode.org/ontologies/pizza/pizza.owl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix owl11: <http://www.w3.org/2006/12/owl11#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl11xml: <http://www.w3.org/2006/12/owl11-xml#> .

pizza:AmericanHot  a     owl:Class ;
        rdfs:label       "AmericanaPicante"@pt ;
        rdfs:subClassOf  pizza:NamedPizza ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      pizza:hasTopping ;
                           owl:someValuesFrom  pizza:MozzarellaTopping
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      pizza:hasTopping ;
                           owl:someValuesFrom  pizza:HotGreenPepperTopping
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      pizza:hasTopping ;
                           owl:someValuesFrom  pizza:JalapenoPepperTopping
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      pizza:hasTopping ;
                           owl:someValuesFrom  pizza:PeperoniSausageTopping
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      pizza:hasTopping ;
                           owl:someValuesFrom  pizza:TomatoTopping
                         ] ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    pizza:America ;
                           owl:onProperty  pizza:hasCountryOfOrigin
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  [ a            owl:Class ;
                                                owl:unionOf  ( pizza:MozzarellaTopping pizza:HotGreenPepperTopping pizza:JalapenoPepperTopping pizza:TomatoTopping pizza:PeperoniSausageTopping )
                                              ] ;
                           owl:onProperty     pizza:hasTopping
                         ] .

pizza:Siciliana  a        owl:Class ;
        rdfs:label        "Siciliana"@pt ;
        rdfs:subClassOf   pizza:NamedPizza ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:GarlicTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:TomatoTopping
                          ] ;
        rdfs:subClassOf   [ a                  owl:Restriction ;
                            owl:allValuesFrom  [ a            owl:Class ;
                                                 owl:unionOf  ( pizza:AnchoviesTopping pizza:OliveTopping pizza:ArtichokeTopping pizza:TomatoTopping pizza:HamTopping pizza:MozzarellaTopping pizza:GarlicTopping )
                                               ] ;
                            owl:onProperty     pizza:hasTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:AnchoviesTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:HamTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:OliveTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:ArtichokeTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:MozzarellaTopping
                          ] ;
        owl:disjointWith  pizza:Giardiniera , pizza:AmericanHot , pizza:UnclosedPizza , pizza:Rosa , pizza:American , pizza:SloppyGiuseppe , pizza:QuattroFormaggi .

pizza:JalapenoPepperTopping
        a                 owl:Class ;
        rdfs:label        "CoberturaDeJalapeno"@pt ;
        rdfs:subClassOf   pizza:PepperTopping ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasSpiciness ;
                            owl:someValuesFrom  pizza:Hot
                          ] ;
        owl:disjointWith  pizza:PeperonataTopping , pizza:SweetPepperTopping , pizza:GreenPepperTopping .

pizza:LeekTopping  a      owl:Class ;
        rdfs:label        "CoberturaDeLeek"@pt ;
        rdfs:subClassOf   pizza:VegetableTopping ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasSpiciness ;
                            owl:someValuesFrom  pizza:Mild
                          ] ;
        owl:disjointWith  pizza:SpinachTopping , pizza:PetitPoisTopping , pizza:AsparagusTopping .

pizza:hasTopping  a         owl:InverseFunctionalProperty , owl:ObjectProperty ;
        rdfs:comment        "Note that hasTopping is inverse functional because isToppingOf is functional"@en ;
        rdfs:domain         pizza:Pizza ;
        rdfs:range          pizza:PizzaTopping ;
        rdfs:subPropertyOf  pizza:hasIngredient ;
        owl:inverseOf       pizza:isToppingOf .

pizza:PeperoniSausageTopping
        a                 owl:Class ;
        rdfs:label        "CoberturaDeCalabreza"@pt ;
        rdfs:subClassOf   pizza:MeatTopping ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasSpiciness ;
                            owl:someValuesFrom  pizza:Medium
                          ] ;
        owl:disjointWith  pizza:ChickenTopping , pizza:HotSpicedBeefTopping , pizza:HamTopping .

pizza:HamTopping  a       owl:Class ;
        rdfs:label        "CoberturaDePresunto"@pt ;
        rdfs:subClassOf   pizza:MeatTopping ;
        owl:disjointWith  pizza:HotSpicedBeefTopping .

pizza:FourCheesesTopping
        a                owl:Class ;
        rdfs:label       "CoberturaQuatroQueijos"@pt ;
        rdfs:subClassOf  pizza:CheeseTopping ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      pizza:hasSpiciness ;
                           owl:someValuesFrom  pizza:Mild
                         ] .

pizza:FruttiDiMare  a     owl:Class ;
        rdfs:label        "FrutosDoMar"@pt ;
        rdfs:subClassOf   pizza:NamedPizza ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:GarlicTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:MixedSeafoodTopping
                          ] ;
        rdfs:subClassOf   [ a                  owl:Restriction ;
                            owl:allValuesFrom  [ a            owl:Class ;
                                                 owl:unionOf  ( pizza:MixedSeafoodTopping pizza:TomatoTopping pizza:GarlicTopping )
                                               ] ;
                            owl:onProperty     pizza:hasTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:TomatoTopping
                          ] ;
        owl:disjointWith  pizza:Margherita , pizza:PolloAdAstra , pizza:QuattroFormaggi , pizza:Napoletana , pizza:Soho , pizza:SloppyGiuseppe , pizza:Rosa , pizza:Veneziana , pizza:American , pizza:UnclosedPizza , pizza:Giardiniera , pizza:AmericanHot , pizza:Siciliana .

pizza:Hot  a             owl:Class ;
        rdfs:label       "Picante"@pt ;
        rdfs:subClassOf  pizza:Spiciness .

pizza:GorgonzolaTopping
        a                 owl:Class ;
        rdfs:label        "CoberturaDeGorgonzola"@pt ;
        rdfs:subClassOf   pizza:CheeseTopping ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasSpiciness ;
                            owl:someValuesFrom  pizza:Mild
                          ] ;
        owl:disjointWith  pizza:FourCheesesTopping , pizza:ParmesanTopping .

pizza:VegetarianPizzaEquivalent1
        a                    owl:Class ;
        rdfs:comment         "Any pizza that only has vegetarian toppings or no toppings is a VegetarianPizzaEquiv1. Should be inferred to be equivalent to VegetarianPizzaEquiv2.  Not equivalent to VegetarianPizza because PizzaTopping is not covering"@en ;
        rdfs:label           "PizzaVegetarianaEquivalente1"@pt ;
        owl:equivalentClass  [ a                   owl:Class ;
                               owl:intersectionOf  ( pizza:Pizza
                                                     [ a                  owl:Restriction ;
                                                       owl:allValuesFrom  pizza:VegetarianTopping ;
                                                       owl:onProperty     pizza:hasTopping
                                                     ]
                                                   )
                             ] .

pizza:PrinceCarlo  a      owl:Class ;
        rdfs:label        "CoberturaPrinceCarlo"@pt ;
        rdfs:subClassOf   pizza:NamedPizza ;
        rdfs:subClassOf   [ a                  owl:Restriction ;
                            owl:allValuesFrom  [ a            owl:Class ;
                                                 owl:unionOf  ( pizza:ParmesanTopping pizza:LeekTopping pizza:RosemaryTopping pizza:TomatoTopping pizza:MozzarellaTopping )
                                               ] ;
                            owl:onProperty     pizza:hasTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:TomatoTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:RosemaryTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:LeekTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:ParmesanTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:MozzarellaTopping
                          ] ;
        owl:disjointWith  pizza:Giardiniera , pizza:Fiorentina , pizza:Napoletana , pizza:SloppyGiuseppe , pizza:FruttiDiMare , pizza:Parmense , pizza:Siciliana , pizza:AmericanHot , pizza:Veneziana , pizza:American , pizza:Margherita , pizza:LaReine , pizza:Soho , pizza:Capricciosa , pizza:QuattroFormaggi , pizza:Rosa , pizza:PolloAdAstra , pizza:UnclosedPizza .

pizza:France  a  owl:Thing , pizza:Country .

pizza:QuattroFormaggi
        a                 owl:Class ;
        rdfs:label        "QuatroQueijos"@pt ;
        rdfs:subClassOf   pizza:NamedPizza ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:TomatoTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:FourCheesesTopping
                          ] ;
        rdfs:subClassOf   [ a                  owl:Restriction ;
                            owl:allValuesFrom  [ a            owl:Class ;
                                                 owl:unionOf  ( pizza:TomatoTopping pizza:FourCheesesTopping )
                                               ] ;
                            owl:onProperty     pizza:hasTopping
                          ] ;
        owl:disjointWith  pizza:AmericanHot .

pizza:VegetableTopping
        a                 owl:Class ;
        rdfs:label        "CoberturaDeVegetais"@pt ;
        rdfs:subClassOf   pizza:PizzaTopping ;
        owl:disjointWith  pizza:FishTopping , pizza:CheeseTopping , pizza:FruitTopping , pizza:HerbSpiceTopping , pizza:MeatTopping .

pizza:RocketTopping  a    owl:Class ;
        rdfs:label        "CoberturaRocket"@pt ;
        rdfs:subClassOf   pizza:VegetableTopping ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasSpiciness ;
                            owl:someValuesFrom  pizza:Medium
                          ] ;
        owl:disjointWith  pizza:SpinachTopping , pizza:MushroomTopping , pizza:TomatoTopping , pizza:ArtichokeTopping , pizza:LeekTopping , pizza:OnionTopping , pizza:PetitPoisTopping , pizza:AsparagusTopping , pizza:OliveTopping .

pizza:HerbSpiceTopping
        a                 owl:Class ;
        rdfs:label        "CoberturaDeErvas"@pt ;
        rdfs:subClassOf   pizza:PizzaTopping ;
        owl:disjointWith  pizza:FishTopping , pizza:MeatTopping , pizza:CheeseTopping .

pizza:FourSeasons  a      owl:Class ;
        rdfs:label        "QuatroQueijos"@pt ;
        rdfs:subClassOf   pizza:NamedPizza ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:MushroomTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:OliveTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:TomatoTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:PeperoniSausageTopping
                          ] ;
        rdfs:subClassOf   [ a                  owl:Restriction ;
                            owl:allValuesFrom  [ a            owl:Class ;
                                                 owl:unionOf  ( pizza:MozzarellaTopping pizza:PeperoniSausageTopping pizza:AnchoviesTopping pizza:CaperTopping pizza:OliveTopping pizza:MushroomTopping pizza:TomatoTopping )
                                               ] ;
                            owl:onProperty     pizza:hasTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:CaperTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:AnchoviesTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:MozzarellaTopping
                          ] ;
        owl:disjointWith  pizza:Parmense , pizza:UnclosedPizza , pizza:Fiorentina , pizza:PolloAdAstra , pizza:Giardiniera , pizza:Margherita , pizza:American , pizza:Siciliana , pizza:LaReine , pizza:Veneziana , pizza:Mushroom , pizza:QuattroFormaggi , pizza:Rosa , pizza:FruttiDiMare , pizza:Caprina , pizza:PrinceCarlo , pizza:Napoletana , pizza:Soho , pizza:SloppyGiuseppe , pizza:Capricciosa , pizza:AmericanHot .

pizza:NutTopping  a       owl:Class ;
        rdfs:label        "CoberturaDeCastanha"@pt ;
        rdfs:subClassOf   pizza:PizzaTopping ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasSpiciness ;
                            owl:someValuesFrom  pizza:Mild
                          ] ;
        owl:disjointWith  pizza:FruitTopping , pizza:VegetableTopping , pizza:CheeseTopping , pizza:MeatTopping , pizza:FishTopping , pizza:HerbSpiceTopping .

pizza:MozzarellaTopping
        a                 owl:Class ;
        rdfs:label        "CoberturaDeMozzarella"@pt ;
        rdfs:subClassOf   pizza:CheeseTopping ;
        rdfs:subClassOf   [ a               owl:Restriction ;
                            owl:hasValue    pizza:Italy ;
                            owl:onProperty  pizza:hasCountryOfOrigin
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasSpiciness ;
                            owl:someValuesFrom  pizza:Mild
                          ] ;
        owl:disjointWith  pizza:GorgonzolaTopping , pizza:GoatsCheeseTopping , pizza:FourCheesesTopping , pizza:ParmesanTopping .

<http://www.co-ode.org/ontologies/pizza/pizza.owl>
        a                owl:Ontology ;
        rdfs:comment     "An example ontology that contains all constructs required for the various versions of the Pizza Tutorial run by Manchester University (see http://www.co-ode.org/resources/tutorials/)"@en ;
        owl:versionInfo  "v.1.4. Added Food class (used in domain/range of hasIngredient), Added several hasCountryOfOrigin restrictions on pizzas, Made hasTopping invers functional"@en , "version 1.5" , "v.1.5. Removed protege.owl import and references. Made ontology URI date-independent"@en .

pizza:SpicyPizza  a          owl:Class ;
        rdfs:comment         "Any pizza that has a spicy topping is a SpicyPizza"@en ;
        rdfs:label           "PizzaTemperada"@pt ;
        owl:equivalentClass  [ a                   owl:Class ;
                               owl:intersectionOf  ( pizza:Pizza
                                                     [ a                   owl:Restriction ;
                                                       owl:onProperty      pizza:hasTopping ;
                                                       owl:someValuesFrom  pizza:SpicyTopping
                                                     ]
                                                   )
                             ] .

[ a                    owl:AllDifferent ;
  owl:distinctMembers  ( pizza:America pizza:Italy pizza:Germany pizza:England pizza:France )
] .

pizza:DeepPanBase  a     owl:Class ;
        rdfs:label       "BaseEspessa"@pt ;
        rdfs:subClassOf  pizza:PizzaBase .

pizza:Food  a            owl:Class ;
        rdfs:subClassOf  pizza:DomainConcept .

pizza:AnchoviesTopping
        a                 owl:Class ;
        rdfs:label        "CoberturaDeAnchovies"@pt ;
        rdfs:subClassOf   pizza:FishTopping ;
        owl:disjointWith  pizza:MixedSeafoodTopping , pizza:PrawnsTopping .

pizza:CaperTopping  a     owl:Class ;
        rdfs:label        "CoberturaDeCaper"@pt ;
        rdfs:subClassOf   pizza:VegetableTopping ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasSpiciness ;
                            owl:someValuesFrom  pizza:Mild
                          ] ;
        owl:disjointWith  pizza:MushroomTopping , pizza:ArtichokeTopping , pizza:LeekTopping , pizza:SpinachTopping , pizza:PetitPoisTopping , pizza:TomatoTopping , pizza:OliveTopping , pizza:AsparagusTopping , pizza:OnionTopping , pizza:RocketTopping , pizza:PepperTopping .

[ a                    owl:AllDifferent ;
  owl:distinctMembers  ( pizza:America pizza:Italy pizza:Germany pizza:England pizza:France )
] .

pizza:OnionTopping  a     owl:Class ;
        rdfs:label        "CoberturaDeCebola"@pt ;
        rdfs:subClassOf   pizza:VegetableTopping ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasSpiciness ;
                            owl:someValuesFrom  pizza:Medium
                          ] ;
        owl:disjointWith  pizza:LeekTopping , pizza:SpinachTopping , pizza:MushroomTopping , pizza:AsparagusTopping , pizza:PetitPoisTopping .

pizza:FishTopping  a      owl:Class ;
        rdfs:label        "CoberturaDePeixe"@pt ;
        rdfs:subClassOf   pizza:PizzaTopping ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasSpiciness ;
                            owl:someValuesFrom  pizza:Mild
                          ] ;
        owl:disjointWith  pizza:CheeseTopping .

pizza:CheeseyVegetableTopping
        a                owl:Class ;
        rdfs:comment     "This class will be inconsistent. This is because we have given it 2 disjoint parents, which means it could never have any members (as nothing can simultaneously be a CheeseTopping and a VegetableTopping). NB Called ProbeInconsistentTopping in the ProtegeOWL Tutorial."@en ;
        rdfs:label       "CoberturaDeQueijoComVegetais"@pt ;
        rdfs:subClassOf  pizza:VegetableTopping , pizza:CheeseTopping .

pizza:Parmense  a         owl:Class ;
        rdfs:label        "Parmense"@pt ;
        rdfs:subClassOf   pizza:NamedPizza ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:ParmesanTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:TomatoTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:HamTopping
                          ] ;
        rdfs:subClassOf   [ a                  owl:Restriction ;
                            owl:allValuesFrom  [ a            owl:Class ;
                                                 owl:unionOf  ( pizza:HamTopping pizza:ParmesanTopping pizza:TomatoTopping pizza:AsparagusTopping pizza:MozzarellaTopping )
                                               ] ;
                            owl:onProperty     pizza:hasTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:AsparagusTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasTopping ;
                            owl:someValuesFrom  pizza:MozzarellaTopping
                          ] ;
        owl:disjointWith  pizza:Napoletana , pizza:SloppyGiuseppe , pizza:AmericanHot , pizza:Rosa , pizza:Siciliana , pizza:QuattroFormaggi , pizza:American , pizza:LaReine , pizza:FruttiDiMare , pizza:Giardiniera , pizza:UnclosedPizza , pizza:Veneziana , pizza:Margherita , pizza:Soho , pizza:Fiorentina , pizza:PolloAdAstra .

pizza:GoatsCheeseTopping
        a                 owl:Class ;
        rdfs:label        "CoberturaDeQueijoDeCabra"@pt ;
        rdfs:subClassOf   pizza:CheeseTopping ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasSpiciness ;
                            owl:someValuesFrom  pizza:Mild
                          ] ;
        owl:disjointWith  pizza:FourCheesesTopping , pizza:GorgonzolaTopping , pizza:ParmesanTopping .

pizza:isBaseOf  a           owl:FunctionalProperty , owl:InverseFunctionalProperty , owl:ObjectProperty ;
        rdfs:domain         pizza:PizzaBase ;
        rdfs:range          pizza:Pizza ;
        rdfs:subPropertyOf  pizza:isIngredientOf .

[ a                    owl:AllDifferent ;
  owl:distinctMembers  ( pizza:America pizza:Italy pizza:Germany pizza:England pizza:France )
] .

pizza:ChickenTopping  a   owl:Class ;
        rdfs:label        "CoberturaDeFrango"@pt ;
        rdfs:subClassOf   pizza:MeatTopping ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasSpiciness ;
                            owl:someValuesFrom  pizza:Mild
                          ] ;
        owl:disjointWith  pizza:HotSpicedBeefTopping , pizza:HamTopping .

pizza:TomatoTopping  a    owl:Class ;
        rdfs:label        "CoberturaDeTomate"@pt ;
        rdfs:subClassOf   pizza:VegetableTopping ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      pizza:hasSpiciness ;
                            owl:someValuesFrom  pizza:Mild
                          ] ;
        owl:disjointWith  pizza:LeekTopping , pizza:OnionTopping , pizza:AsparagusTopping , pizza:SpinachTopping , pizza:ArtichokeTopping , pizza:MushroomTopping , pizza:PetitPoisTopping .

pizza:HotGreenPepperTopping
        a                owl:Class ;
        rdfs:label       "CoberturaDePimentaoVerdePicante"@pt ;
        rdfs:subClassOf  pizza:GreenPepperTopping ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      pizza:hasSpiciness ;
                           owl:someValuesFrom  pizza:Hot
                         ] .

pizza:VegetarianPizzaEquivalent2
        a                    owl:Class ;
        rdfs:comment         "An alternative to VegetarianPizzaEquiv1 that does not require a definition of VegetarianTopping. Perhaps more difficult to maintain. Not equivalent to VegetarianPizza"@en ;
        rdfs:label           "PizzaVegetarianaEquivalente2"@pt ;
        owl:equivalentClass  [ a                   owl:Class ;
                               owl:intersectionOf  ( [ a                  owl:Restriction ;
                                                       owl:allValuesFrom  [ a            owl:Class ;
                                                                            owl:unionOf  ( pizza:SauceTopping pizza:VegetableTopping pizza:FruitTopping pizza:CheeseTopping pizza:HerbSpiceTopping pizza:NutTopping )
                                                                          ] ;
                                                       owl:onProperty     pizza:hasTopping
                                                     ]
                                                     pizza:Pizza
                                                   )
                             ] .

pizza:FruitTopping  a     owl:Class ;
        rdfs:label        "CoberturaDeFrutas"@pt ;
        rdfs:subClassOf   pizza:PizzaTopping ;
        owl:disjointWith  pizza:CheeseTopping , pizza:FishTopping , pizza:HerbSpiceTopping , pizza:MeatTopping .

pizza:Napoletana  a       owl:Class ;
        rdfs:label        "Napoletana"@pt ;
